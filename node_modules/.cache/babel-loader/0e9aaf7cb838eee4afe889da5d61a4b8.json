AAA,cADA;AAEA,IAAA,aAAA,EAAA;AAFA,GAFA;AAMA,EAAA,KAAA,EAAA;AACA,IAAA,OAAA,EAAA,KADA;AAEA,IAAA,UAAA,EAAA;AAFA,GANA;AAUA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AACA,UAAA,OAAA,GAAA,EAAA;AACA,UAAA,SAAA,GAAA,OAAA;;AAEA,UAAA,OAAA,KAAA,UAAA,KAAA,WAAA,EAAA;AACA,YAAA,KAAA,UAAA,CAAA,cAAA,CAAA,WAAA,CAAA,EAAA;AACA,UAAA,SAAA,GAAA,KAAA,UAAA,CAAA,SAAA;AACA,SAHA,CAKA;;;AACA,YAAA,KAAA,UAAA,EAAA;AACA,UAAA,OAAA,CAAA,IAAA,CAAA,yBAAA;AACA;AACA;;AAEA,UAAA,CAAA,KAAA,UAAA,EAAA;AACA,QAAA,OAAA,CAAA,IAAA,CAAA,2BAAA;AACA;;AAEA,UAAA,CAAA,KAAA,UAAA,EAAA;AACA,QAAA,OAAA,CAAA,IAAA,6BAAA,SAAA;AACA;;AAEA,aAAA,OAAA,CAAA,IAAA,CAAA,GAAA,CAAA;AACA,KAzBA;;AA0BA;;;;;AAKA,IAAA,aA/BA,yBA+BA,IA/BA,EA+BA;AACA,UAAA,IAAA,CAAA,cAAA,CAAA,QAAA,CAAA,EAAA;AACA,eAAA,IAAA,CAAA,MAAA;AACA;;AACA,aAAA,OAAA;AACA,KApCA;AAqCA,IAAA,UArCA,sBAqCA,KArCA,EAqCA;AAAA;;AACA,UAAA,KAAA,GAAA,KAAA,CAAA,OAAA,CAAA,KAAA,IAAA,KAAA,GAAA,CAAA,KAAA,CAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,OAAA,EAAA;AACA,iBAAA,KAAA,CAAA,UAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA;;AACA,YAAA,IAAA,CAAA,IAAA,EAAA;AACA;AACA,iBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,gBAAA,IAAA,CAAA,IAAA,CAAA;AACA;AACA,OARA,CAAA;AASA;AAhDA,GAVA;AA4DA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,OAAA,KAAA,UAAA,KAAA,WAAA,EAAA;AACA,eACA,KAAA,UAAA,CAAA,cAAA,CAAA,MAAA,KACA,KAAA,UAAA,CAAA,IAAA,KAAA,MAFA;AAIA;;AACA,aAAA,KAAA;AACA;AATA;AA5DA,CAAA","sourcesContent":["<template>\r\n  <div class=\"kt-menu__submenu\" :class=\"submenuClass()\">\r\n    <ul v-if=\"!isMegaMenu\" class=\"kt-menu__subnav\">\r\n      <template v-for=\"(menu, i) in submenu\">\r\n        <li\r\n          class=\"kt-menu__item\"\r\n          aria-haspopup=\"true\"\r\n          :key=\"i\"\r\n          :class=\"{\r\n            'kt-menu__item--submenu': menu.submenu,\r\n            'kt-menu__item--active': activeMenu(menu)\r\n          }\"\r\n          :data-ktmenu-submenu-toggle=\"submenuToggle(menu)\"\r\n        >\r\n          <KTMenuItemText\r\n            v-bind:menu=\"menu\"\r\n            v-bind:parentMenu=\"parentMenu\"\r\n          ></KTMenuItemText>\r\n          <KTMenuSubmenu\r\n            v-if=\"menu.submenu\"\r\n            v-bind:submenu=\"menu.submenu\"\r\n          ></KTMenuSubmenu>\r\n        </li>\r\n      </template>\r\n    </ul>\r\n\r\n    <div\r\n      v-if=\"isMegaMenu\"\r\n      class=\"kt-menu__subnav\"\r\n      :style=\"{ width: `${submenu.length * 200}px` }\"\r\n    >\r\n      <ul class=\"kt-menu__content\">\r\n        <template v-for=\"(column, i) in submenu\">\r\n          <li class=\"kt-menu__item\" :key=\"i\">\r\n            <!-- column title -->\r\n            <h3 v-if=\"column.title\" class=\"kt-menu__heading kt-menu__toggle\">\r\n              <span class=\"kt-menu__link-text\">{{ column.title }}</span>\r\n            </h3>\r\n\r\n            <!-- column inner menu -->\r\n            <ul class=\"kt-menu__inner\">\r\n              <template v-for=\"(menu, i) in column.submenu\">\r\n                <li\r\n                  class=\"kt-menu__item\"\r\n                  aria-haspopup=\"true\"\r\n                  :key=\"i\"\r\n                  :class=\"{ 'kt-menu__item--active': activeMenu(menu) }\"\r\n                >\r\n                  <KTMenuItemText\r\n                    v-bind:menu=\"menu\"\r\n                    v-bind:parentMenu=\"column\"\r\n                  ></KTMenuItemText>\r\n                </li>\r\n              </template>\r\n            </ul>\r\n          </li>\r\n        </template>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport KTMenuItemText from \"@/views/theme/header/MenuItemText.vue\";\r\nimport KTMenuSubmenu from \"@/views/theme/header/MenuSubmenu.vue\";\r\n\r\nexport default {\r\n  name: \"KTMenuSubmenu\",\r\n  components: {\r\n    KTMenuItemText,\r\n    KTMenuSubmenu\r\n  },\r\n  props: {\r\n    submenu: Array,\r\n    parentMenu: Object\r\n  },\r\n  methods: {\r\n    submenuClass() {\r\n      const classes = [];\r\n      var alignment = \"right\";\r\n\r\n      if (typeof this.parentMenu !== \"undefined\") {\r\n        if (this.parentMenu.hasOwnProperty(\"alignment\")) {\r\n          alignment = this.parentMenu.alignment;\r\n        }\r\n\r\n        // classic or mega menu\r\n        if (this.isMegaMenu) {\r\n          classes.push(\"kt-menu__submenu--fixed\");\r\n        }\r\n      }\r\n\r\n      if (!this.isMegaMenu) {\r\n        classes.push(\"kt-menu__submenu--classic\");\r\n      }\r\n\r\n      if (!this.parentMenu) {\r\n        classes.push(`kt-menu__submenu--${alignment}`);\r\n      }\r\n\r\n      return classes.join(\" \");\r\n    },\r\n    /**\r\n     * Get submenu toggle type\r\n     * @param menu\r\n     * @returns {string|*}\r\n     */\r\n    submenuToggle(menu) {\r\n      if (menu.hasOwnProperty(\"toggle\")) {\r\n        return menu.toggle;\r\n      }\r\n      return \"hover\";\r\n    },\r\n    activeMenu(input) {\r\n      const paths = Array.isArray(input) ? input : [input];\r\n      return paths.some(conf => {\r\n        if (conf.submenu) {\r\n          return this.activeMenu(conf.submenu);\r\n        }\r\n        if (conf.page) {\r\n          // current path starts with this path string\r\n          return this.$route.path === `/${conf.page}`;\r\n        }\r\n      });\r\n    }\r\n  },\r\n  computed: {\r\n    isMegaMenu() {\r\n      if (typeof this.parentMenu !== \"undefined\") {\r\n        return (\r\n          this.parentMenu.hasOwnProperty(\"type\") &&\r\n          this.parentMenu.type === \"mega\"\r\n        );\r\n      }\r\n      return false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/theme/header"}]}