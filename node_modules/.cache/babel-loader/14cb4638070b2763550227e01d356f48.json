;AAFA,OADA;AAKA,MAAA,QAAA,EAAA;AACA,QAAA,QAAA,EAAA,QADA;AAEA,QAAA,SAAA,EAAA,SAAA,CAAA,CAAA;AAFA;AALA;AADA,GAZA;AAwBA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,yBACA,IADA,EACA;AAAA,+BACA,KAAA,EAAA,CAAA,IAAA,CAAA,IAAA,CADA;AAAA,UACA,MADA,sBACA,MADA;AAAA,UACA,MADA,sBACA,MADA;AAEA,aAAA,MAAA,GAAA,CAAA,MAAA,GAAA,IAAA;AACA,KAJA;AAKA,IAAA,SALA,uBAKA;AAAA;;AACA,WAAA,IAAA,GAAA;AACA,QAAA,KAAA,EAAA,IADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAAA;AAKA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,EAAA,CAAA,MAAA;AACA,OAFA;AAGA,KAdA;AAeA,IAAA,QAfA,sBAeA;AAAA;;AACA,WAAA,EAAA,CAAA,IAAA,CAAA,MAAA;;AACA,UAAA,KAAA,EAAA,CAAA,IAAA,CAAA,SAAA,EAAA;AACA;AACA;;AAEA,UAAA,KAAA,GAAA,KAAA,EAAA,CAAA,IAAA,CAAA,KAAA,CAAA,MAAA;AACA,UAAA,QAAA,GAAA,KAAA,EAAA,CAAA,IAAA,CAAA,QAAA,CAAA,MAAA,CAPA,CASA;;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,MAAA,EAVA,CAYA;;AACA,UAAA,YAAA,GAAA,QAAA,CAAA,cAAA,CAAA,WAAA,CAAA;AACA,MAAA,YAAA,CAAA,SAAA,CAAA,GAAA,CACA,YADA,EAEA,mBAFA,EAGA,mBAHA,EAdA,CAoBA;;AACA,MAAA,UAAA,CAAA,YAAA;AACA;AACA,QAAA,MAAA,CAAA,MAAA,CACA,QADA,CACA,KADA,EACA;AAAA,UAAA,KAAA,EAAA,KAAA;AAAA,UAAA,QAAA,EAAA;AAAA,SADA,EAEA;AAFA,SAGA,IAHA,CAGA;AAAA,iBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA,CAAA;AAAA,SAHA;;AAKA,QAAA,YAAA,CAAA,SAAA,CAAA,MAAA,CACA,YADA,EAEA,mBAFA,EAGA,mBAHA;AAKA,OAZA,EAYA,IAZA,CAAA;AAaA;AAjDA,GAxBA;AA2EA,EAAA,QAAA,kCACA,QAAA,CAAA;AACA,IAAA,MAAA,EAAA,gBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,IAAA,CAAA,MAAA;AAAA;AADA,GAAA,CADA;AAIA,IAAA,eAJA,6BAIA;AACA,aAAA,OAAA,CAAA,GAAA,CAAA,QAAA,GAAA,0BAAA;AACA;AANA;AA3EA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!--begin::Head-->\r\n    <div class=\"kt-login__head\">\r\n      <span class=\"kt-login__signup-label\">{{\r\n        $t(\"AUTH.GENERAL.NO_ACCOUNT\")\r\n      }}</span\r\n      >&nbsp;&nbsp;\r\n      <router-link\r\n        class=\"kt-link kt-login__signup-link\"\r\n        :to=\"{ name: 'register' }\"\r\n      >\r\n        {{ $t(\"AUTH.GENERAL.SIGNUP_BUTTON\") }}\r\n      </router-link>\r\n    </div>\r\n    <!--end::Head-->\r\n\r\n    <!--begin::Body-->\r\n    <div class=\"kt-login__body\">\r\n      <!--begin::Signin-->\r\n      <div class=\"kt-login__form\">\r\n        <div class=\"kt-login__title\">\r\n          <h3>Sign In</h3>\r\n        </div>\r\n\r\n        <!--begin::Form-->\r\n        <b-form class=\"kt-form\" @submit.stop.prevent=\"onSubmit\">\r\n          <div role=\"alert\" class=\"alert alert-info\">\r\n            <div class=\"alert-text\">\r\n              Use account <strong>admin@demo.com</strong> and password\r\n              <strong>demo</strong> to continue.\r\n            </div>\r\n          </div>\r\n\r\n          <div\r\n            role=\"alert\"\r\n            v-bind:class=\"{ show: errors.length }\"\r\n            class=\"alert fade alert-danger\"\r\n          >\r\n            <div class=\"alert-text\" v-for=\"(error, i) in errors\" :key=\"i\">\r\n              {{ error }}\r\n            </div>\r\n          </div>\r\n\r\n          <b-form-group\r\n            id=\"example-input-group-1\"\r\n            label=\"\"\r\n            label-for=\"example-input-1\"\r\n          >\r\n            <b-form-input\r\n              id=\"example-input-1\"\r\n              name=\"example-input-1\"\r\n              v-model=\"$v.form.email.$model\"\r\n              :state=\"validateState('email')\"\r\n              aria-describedby=\"input-1-live-feedback\"\r\n            ></b-form-input>\r\n\r\n            <b-form-invalid-feedback id=\"input-1-live-feedback\">\r\n              Email is required and a valid email address.\r\n            </b-form-invalid-feedback>\r\n          </b-form-group>\r\n\r\n          <b-form-group\r\n            id=\"example-input-group-2\"\r\n            label=\"\"\r\n            label-for=\"example-input-2\"\r\n          >\r\n            <b-form-input\r\n              type=\"password\"\r\n              id=\"example-input-2\"\r\n              name=\"example-input-2\"\r\n              v-model=\"$v.form.password.$model\"\r\n              :state=\"validateState('password')\"\r\n              aria-describedby=\"input-2-live-feedback\"\r\n            ></b-form-input>\r\n\r\n            <b-form-invalid-feedback id=\"input-2-live-feedback\">\r\n              Password is required.\r\n            </b-form-invalid-feedback>\r\n          </b-form-group>\r\n\r\n          <!--begin::Action-->\r\n          <div class=\"kt-login__actions\">\r\n            <a href=\"#\" class=\"kt-link kt-login__link-forgot\">\r\n              {{ $t(\"AUTH.FORGOT.TITLE\") }}\r\n            </a>\r\n            <b-button\r\n              type=\"submit\"\r\n              id=\"kt_submit\"\r\n              class=\"btn btn-primary btn-elevate kt-login__btn-primary\"\r\n            >\r\n              {{ $t(\"AUTH.LOGIN.BUTTON\") }}\r\n            </b-button>\r\n          </div>\r\n          <!--end::Action-->\r\n        </b-form>\r\n        <!--end::Form-->\r\n\r\n        <!--begin::Divider-->\r\n        <div class=\"kt-login__divider\">\r\n          <div class=\"kt-divider\">\r\n            <span></span>\r\n            <span>OR</span>\r\n            <span></span>\r\n          </div>\r\n        </div>\r\n        <!--end::Divider-->\r\n\r\n        <!--begin::Options-->\r\n        <div class=\"kt-login__options\">\r\n          <a href=\"#\" class=\"btn btn-primary kt-btn\">\r\n            <i class=\"fab fa-facebook-f\"></i>\r\n            Facebook\r\n          </a>\r\n\r\n          <a href=\"#\" class=\"btn btn-info kt-btn\">\r\n            <i class=\"fab fa-twitter\"></i>\r\n            Twitter\r\n          </a>\r\n\r\n          <a href=\"#\" class=\"btn btn-danger kt-btn\">\r\n            <i class=\"fab fa-google\"></i>\r\n            Google\r\n          </a>\r\n        </div>\r\n        <!--end::Options-->\r\n      </div>\r\n      <!--end::Signin-->\r\n    </div>\r\n    <!--end::Body-->\r\n  </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n.kt-spinner.kt-spinner--right:before {\r\n  right: 8px;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport { mapState } from \"vuex\";\r\nimport { LOGIN, LOGOUT } from \"@/store/auth.module\";\r\n\r\nimport { validationMixin } from \"vuelidate\";\r\nimport { email, minLength, required } from \"vuelidate/lib/validators\";\r\n\r\nexport default {\r\n  mixins: [validationMixin],\r\n  name: \"login\",\r\n  data() {\r\n    return {\r\n      // Remove this dummy login info\r\n      form: {\r\n        email: \"admin@demo.com\",\r\n        password: \"demo\"\r\n      }\r\n    };\r\n  },\r\n  validations: {\r\n    form: {\r\n      email: {\r\n        required,\r\n        email\r\n      },\r\n      password: {\r\n        required,\r\n        minLength: minLength(3)\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    validateState(name) {\r\n      const { $dirty, $error } = this.$v.form[name];\r\n      return $dirty ? !$error : null;\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        email: null,\r\n        password: null\r\n      };\r\n\r\n      this.$nextTick(() => {\r\n        this.$v.$reset();\r\n      });\r\n    },\r\n    onSubmit() {\r\n      this.$v.form.$touch();\r\n      if (this.$v.form.$anyError) {\r\n        return;\r\n      }\r\n\r\n      const email = this.$v.form.email.$model;\r\n      const password = this.$v.form.password.$model;\r\n\r\n      // clear existing errors\r\n      this.$store.dispatch(LOGOUT);\r\n\r\n      // set spinner to submit button\r\n      const submitButton = document.getElementById(\"kt_submit\");\r\n      submitButton.classList.add(\r\n        \"kt-spinner\",\r\n        \"kt-spinner--light\",\r\n        \"kt-spinner--right\"\r\n      );\r\n\r\n      // dummy delay\r\n      setTimeout(() => {\r\n        // send login request\r\n        this.$store\r\n          .dispatch(LOGIN, { email, password })\r\n          // go to which page after successfully login\r\n          .then(() => this.$router.push({ name: \"dashboard\" }));\r\n\r\n        submitButton.classList.remove(\r\n          \"kt-spinner\",\r\n          \"kt-spinner--light\",\r\n          \"kt-spinner--right\"\r\n        );\r\n      }, 2000);\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      errors: state => state.auth.errors\r\n    }),\r\n    backgroundImage() {\r\n      return process.env.BASE_URL + \"assets/media/bg/bg-4.jpg\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/pages/auth"}]}