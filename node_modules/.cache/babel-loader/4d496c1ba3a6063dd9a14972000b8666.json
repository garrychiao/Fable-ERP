n              </b-card>\r\n\r\n              <b-card no-body class=\"mb-1\">\r\n                <b-card-header header-tag=\"header\" class=\"p-1\" role=\"tab\">\r\n                  <b-button block href=\"#\" v-b-toggle.accordion-2 variant=\"info\"\r\n                    >Accordion 2</b-button\r\n                  >\r\n                </b-card-header>\r\n                <b-collapse\r\n                  id=\"accordion-2\"\r\n                  accordion=\"my-accordion\"\r\n                  role=\"tabpanel\"\r\n                >\r\n                  <b-card-body>\r\n                    <b-card-text>{{ text }}</b-card-text>\r\n                  </b-card-body>\r\n                </b-collapse>\r\n              </b-card>\r\n\r\n              <b-card no-body class=\"mb-1\">\r\n                <b-card-header header-tag=\"header\" class=\"p-1\" role=\"tab\">\r\n                  <b-button block href=\"#\" v-b-toggle.accordion-3 variant=\"info\"\r\n                    >Accordion 3</b-button\r\n                  >\r\n                </b-card-header>\r\n                <b-collapse\r\n                  id=\"accordion-3\"\r\n                  accordion=\"my-accordion\"\r\n                  role=\"tabpanel\"\r\n                >\r\n                  <b-card-body>\r\n                    <b-card-text>{{ text }}</b-card-text>\r\n                  </b-card-body>\r\n                </b-collapse>\r\n              </b-card>\r\n            </div>\r\n          </template>\r\n          <template v-slot:html>\r\n            {{ html3 }}\r\n          </template>\r\n        </KTCodePreview>\r\n      </div>\r\n\r\n      <div class=\"col-md-6\">\r\n        <KTCodePreview v-bind:title=\"'Usage'\">\r\n          <template v-slot:preview>\r\n            <p>\r\n              Other elements can easily toggle\r\n              <code>&lt;b-collapse&gt;</code> components using the\r\n              <code></code> directive.\r\n            </p>\r\n            <div>\r\n              <!-- Using modifiers -->\r