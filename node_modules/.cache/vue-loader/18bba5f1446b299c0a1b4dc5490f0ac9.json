e> and <code>per-page</code>.\r\n            </p>\r\n            <div class=\"overflow-auto\">\r\n              <b-pagination\r\n                v-model=\"code1.currentPage\"\r\n                :total-rows=\"rows\"\r\n                :per-page=\"code1.perPage\"\r\n                aria-controls=\"my-table\"\r\n              ></b-pagination>\r\n\r\n              <p class=\"mt-3\">Current Page: {{ code1.currentPage }}</p>\r\n\r\n              <b-table\r\n                id=\"my-table\"\r\n                :items=\"code1.items\"\r\n                :per-page=\"code1.perPage\"\r\n                :current-page=\"code1.currentPage\"\r\n                small\r\n              ></b-table>\r\n            </div>\r\n          </template>\r\n          <template v-slot:html>\r\n            {{ code1.html }}\r\n          </template>\r\n          <template v-slot:js>\r\n            {{ code1.js }}\r\n          </template>\r\n        </KTCodePreview>\r\n\r\n        <KTCodePreview v-bind:title=\"'Button content'\">\r\n          <template v-slot:preview>\r\n            <div class=\"overflow-auto\">\r\n              <!-- Use text in props -->\r\n              <b-pagination\r\n                v-model=\"code2.currentPage1\"\r\n                :total-rows=\"code2.rows1\"\r\n                :per-page=\"code2.perPage1\"\r\n                first-text=\"First\"\r\n                prev-text=\"Prev\"\r\n                next-text=\"Next\"\r\n                last-text=\"Last\"\r\n              ></b-pagination>\r\n\r\n              <!-- Use emojis in props -->\r\n              <b-pagination\r\n                v-model=\"code2.currentPage2\"\r\n                :total-rows=\"code2.rows2\"\r\n                :per-page=\"code2.perPage2\"\r\n                first-text=\"⏮\"\r\n                prev-text=\"⏪\"\r\n                next-text=\"⏩\"\r\n                last-text=\"⏭\"\r\n                class=\"mt-4\"\r\n              ></b-pagination>\r\n\r\n              <!-- Use HTML and sub-components in slots --