tor=\"{ on }\">\r\n                  <v-btn class=\"mr-4\" color=\"primary\" dark v-on=\"on\"\r\n                    >Bottom</v-btn\r\n                  >\r\n                </template>\r\n                <span>Bottom tooltip</span>\r\n              </v-tooltip>\r\n\r\n              <v-tooltip right>\r\n                <template v-slot:activator=\"{ on }\">\r\n                  <v-btn class=\"mr-4\" color=\"primary\" dark v-on=\"on\"\r\n                    >Right</v-btn\r\n                  >\r\n                </template>\r\n                <span>Right tooltip</span>\r\n              </v-tooltip>\r\n            </div>\r\n          </template>\r\n          <template v-slot:html>\r\n            {{ code1.html }}\r\n          </template>\r\n        </KTCodePreview>\r\n      </div>\r\n\r\n      <div class=\"col-md-6\">\r\n        <KTCodePreview v-bind:title=\"'Visibility'\">\r\n          <template v-slot:preview>\r\n            <p>\r\n              Tooltip visibility can be programmatically changed using\r\n              <code>v-model</code>.\r\n            </p>\r\n            <v-container fluid class=\"text-center\">\r\n              <v-row class=\"flex\" justify=\"space-between\">\r\n                <v-col cols=\"12\">\r\n                  <v-btn @click=\"show = !show\">toggle</v-btn>\r\n                </v-col>\r\n\r\n                <v-col cols=\"12\" class=\"mt-12\">\r\n                  <v-tooltip v-model=\"show\" top>\r\n                    <template v-slot:activator=\"{ on }\">\r\n                      <v-btn icon v-on=\"on\">\r\n                        <v-icon color=\"grey lighten-1\">mdi-cart</v-icon>\r\n                      </v-btn>\r\n                    </template>\r\n                    <span>Programmatic tooltip</span>\r\n                  </v-tooltip>\r\n                </v-col>\r\n              </v-row>\r\n            </v-container>\r\n          </template>\r\n          <template v-slot:html>\r\n            {{ code2.html }}\r\n          </template>\r\n