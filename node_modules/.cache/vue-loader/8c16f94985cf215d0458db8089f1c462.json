 class=\"kt-user-card__avatar\">\r\n        <img\r\n          class=\"kt-hidden\"\r\n          alt=\"Pic\"\r\n          src=\"@/assets/media/users/300_25.jpg\"\r\n        />\r\n        <!--use below badge element instead the user avatar to display username's first letter(remove kt-hidden class to display it) -->\r\n        <span\r\n          class=\"kt-badge kt-badge--lg kt-badge--rounded kt-badge--bold kt-font-success\"\r\n          >S</span\r\n        >\r\n      </div>\r\n      <div class=\"kt-user-card__name\">Sean Stone</div>\r\n      <div class=\"kt-user-card__badge\">\r\n        <span class=\"btn btn-success btn-sm btn-bold btn-font-md\"\r\n          >23 messages</span\r\n        >\r\n      </div>\r\n    </div>\r\n    <!--end: Head -->\r\n    <!--begin: Navigation -->\r\n    <div class=\"kt-notification\">\r\n      <a href=\"#\" class=\"kt-notification__item\">\r\n        <div class=\"kt-notification__item-icon\">\r\n          <i class=\"flaticon2-calendar-3 kt-font-success\"></i>\r\n        </div>\r\n        <div class=\"kt-notification__item-details\">\r\n          <div class=\"kt-notification__item-title kt-font-bold\">My Profile</div>\r\n          <div class=\"kt-notification__item-time\">\r\n            Account settings and more\r\n          </div>\r\n        </div>\r\n      </a>\r\n      <a href=\"#\" class=\"kt-notification__item\">\r\n        <div class=\"kt-notification__item-icon\">\r\n          <i class=\"flaticon2-mail kt-font-warning\"></i>\r\n        </div>\r\n        <div class=\"kt-notification__item-details\">\r\n          <div class=\"kt-notification__item-title kt-font-bold\">\r\n            My Messages\r\n          </div>\r\n          <div class=\"kt-notification__item-time\">Inbox and tasks</div>\r\n        </div>\r\n      </a>\r\n      <a href=\"#\" class=\"kt-notification__item\">\r\n        <div class=\"kt-notification__item-icon\">\r\n          <i class=\"flaticon2-rocket-1 kt-font-danger\"></i>\r\n        </div>\r\n        <div class=\"kt-notification__item-details\">\r\n          <div class=\"kt-notification__item-title kt-font-bold\">\r\n            My Activities\r\n          </div>\r\n          <div class=\"kt-notification__item-time\">Logs and notifications</div>\r\n        </div>\r\n      </a>\r\n      <a href=\"#\" class=\"kt-notification__item\">\r\n        <div class=\"kt-notification__item-icon\">\r\n          <i class=\"flaticon2-hourglass kt-font-brand\"></i>\r\n        </div>\r\n        <div class=\"kt-notification__item-details\">\r\n          <div class=\"kt-notification__item-title kt-font-bold\">My Tasks</div>\r\n          <div class=\"kt-notification__item-time\">\r\n            latest tasks and projects\r\n          </div>\r\n        </div>\r\n      </a>\r\n      <a href=\"#\" class=\"kt-notification__item\">\r\n        <div class=\"kt-notification__item-icon\">\r\n          <i class=\"flaticon2-cardiogram kt-font-warning\"></i>\r\n        </div>\r\n        <div class=\"kt-notification__item-details\">\r\n          <div class=\"kt-notification__item-title kt-font-bold\">Billing</div>\r\n          <div class=\"kt-notification__item-time\">\r\n            billing & statements\r\n            <span\r\n              class=\"kt-badge kt-badge--danger kt-badge--inline kt-badge--pill kt-badge--rounded\"\r\n              >2 pending</span\r\n            >\r\n          </div>\r\n        </div>\r\n      </a>\r\n      <div class=\"kt-notification__custom kt-space-between\">\r\n        <a\r\n          href=\"#\"\r\n          v-on:click=\"onLogout()\"\r\n          class=\"btn btn-label btn-label-brand btn-sm btn-bold\"\r\n          >Sign Out</a\r\n        >\r\n        <a\r\n          href=\"#\"\r\n          v-on:click=\"onLogout()\"\r\n          class=\"btn btn-clean btn-sm btn-bold\"\r\n          >Upgrade Plan</a\r\n        >\r\n      </div>\r\n    </div>\r\n    <!--end: Navigation -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { LOGOUT } from \"@/store/auth.module\";\r\n\r\nexport default {\r\n  name: \"KTDropdownUser\",\r\n  methods: {\r\n    onLogout() {\r\n      this.$store\r\n        .dispatch(LOGOUT)\r\n        .then(() => this.$router.push({ name: \"login\" }));\r\n    }\r\n  },\r\n  computed: {\r\n    backgroundImage() {\r\n      return process.env.BASE_URL + \"assets/media/misc/bg-1.jpg\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}